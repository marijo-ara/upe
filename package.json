{
  "name": "upe-store",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx tsc --noEmit",
    "lint:fix": "eslint ./ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "npx eslint .",
    "cypress:open:dev": "npx cypress open --env configFile=dev",
    "cypress:open:qa": "npx cypress open --env configFile=qa",
    "cypress:open:stg": "npx cypress open --env configFile=stg",
    "delete:testing:reports": "rm -r cypress/results_xml/* && rm -r cypress/results_json/*",
    "combine:xml:reports": "jrm cypress/junit-report.xml \"cypress/results_xml/*.xml\"",
    "merge:json:reports": "mochawesome-merge cypress/results_json/*.json > cypress/mochawesome-report.json && marge cypress/mochawesome-report.json --reportDir ./ --inline",
    "cypress:run:dev": "npx cypress run --env configFile=dev --spec 'cypress/e2e/regression/*.cy.js' --browser chrome --e2e --reporter cypress-multi-reporters --headed",
    "cypress:run:qa": "npx cypress run --env configFile=qa --spec 'cypress/e2e/regression/*.cy.js' --browser chrome --e2e --reporter cypress-multi-reporters --headed",
    "smoke:run:prod": "npx cypress run --env configFile=prod --spec 'cypress/e2e/smoke/*.cy.js' --browser chrome --e2e --reporter cypress-multi-reporters --headed",
    "smoke:run:stg": "npx cypress run --env configFile=stg --spec 'cypress/e2e/smoke/*.cy.js' --browser chrome --e2e --reporter cypress-multi-reporters --headed",
    "test:axe": "npx cypress run --spec 'cypress/e2e/axe/*.cy.js' --browser chrome | tee cypress/accessibility.txt"
  },
